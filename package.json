{
    "name": "web",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "tasks": "gulp --tasks --gulpfile node_modules/@dolittle/typescript.build/Gulpfile.js",
        "build": "gulp build --gulpfile node_modules/@dolittle/typescript.build/Gulpfile.js",
        "clean": "gulp clean --gulpfile node_modules/@dolittle/typescript.build/Gulpfile.js",
        "test": "gulp test --gulpfile node_modules/@dolittle/typescript.build/Gulpfile.js",
        "test:run": "gulp test-run --gulpfile node_modules/@dolittle/typescript.build/Gulpfile.js",
        "webpack": "npx webpack --env.production --extractCss",
        "webpack:dev": "npx webpack --extractCss",
        "start": "npx webpack-dev-server --open --watch --progress --hot"
    },
    "devDependencies": {
        "@babel/core": "^7.7.4",
        "@dolittle/build": "^6.3.4",
        "@dolittle/typescript.build": "^5.1.1",
        "@dolittle/typescript.webpack.aurelia": "^1.1.0",
        "dotenv": "^6.1.0",
        "tslint": "^5.20.1"
    },
    "dependencies": {
        "@dolittle/aurelia": "^2.0.0",
        "@types/materialize-css": "^1.0.6",
        "aurelia-materialize-bridge": "^1.4.1",
        "aurelia-typed-observable-plugin": "^0.4.2",
        "aurelia-validation": "^1.5.0",
        "materialize-css": "^1.0.0-rc.2",
        "prettier": "^1.19.1"
    }
}
